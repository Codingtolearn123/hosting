  },
  "content": [
    {
      "id": "hero-section",
      "elType": "section",
      "settings": {
        "structure": "50",
        "background_background": "gradient",
        "background_color": "#0b0f2c",
        "background_gradient_type": "linear",
        "background_gradient_angle": 135,
        "background_gradient_color": "#6c63ff",
        "background_gradient_color_b": "#ff72d2",
        "padding": {
          "unit": "px",
          "top": "120",
          "right": "60",
          "bottom": "120",
          "left": "60",
          "isLinked": false
        },
        "shape_divider_bottom": "tilt",
        "shape_divider_bottom_negative": "yes",
        "shape_divider_bottom_color": "#05081c"
      },
      "elements": [
        {
          "id": "hero-column",
          "elType": "column",
          "settings": {
            "_column_size": 100
          },
          "elements": [
            {
              "id": "hero-badge",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<span class=\"badge\">AI Optimised Cloud</span>"
              },
              "elements": []
            },
            {
              "id": "hero-heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Scale faster with Virtual Sky Hosting",
                "size": "xl"
              },
              "elements": []
            },
            {
              "id": "hero-subheading",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p>High-performance infrastructure with AI automation, backed by 24/7 engineers and seamless WHMCS provisioning.</p>"
              },
              "elements": []
            },
            {
              "id": "hero-buttons",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Get Started",
                "size": "lg",
                "link": {
                  "url": "#",
                  "custom_attributes": "data-whmcs-target=\"web-hosting\""
                }
              },
              "elements": []
            },
            {
              "id": "hero-secondary",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Explore Plans",
                "size": "md",
                "button_type": "ghost",
                "link": {
                  "url": "#plans",
